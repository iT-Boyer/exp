# XXX Read https://github.com/koekeishiya/skhd/blob/master/examples/skhdrc

alt - space : /Users/jay/.jwm/bin/new-termw
cmd - return : /Users/jay/.jwm/bin/new-termw

# XXX Messes with Safari and closing tabs
#cmd - w : chunkc tiling::window --close

cmd + ctrl - t : chunkc tiling::window --toggle float;\
                 chunkc tiling::window --grid-layout 4:4:1:1:2:2

cmd + ctrl - h : chunkc tiling::window --focus west 
cmd + ctrl - l : chunkc tiling::window --focus east
cmd + ctrl - j : chunkc tiling::window --focus south
cmd + ctrl - k : chunkc tiling::window --focus north
cmd + ctrl - i : chunkc tiling::window --focus prev
cmd + ctrl - o : chunkc tiling::window --focus next

cmd + alt - h : chunkc tiling::window --swap west
cmd + alt - l : chunkc tiling::window --swap east
cmd + alt - j : chunkc tiling::window --swap south
cmd + alt - k : chunkc tiling::window --swap north
cmd + alt - i : chunkc tiling::window --swap prev
cmd + alt - o : chunkc tiling::window --swap next

cmd + shift - 0 : chunkc tiling::window --send-to-desktop 0
cmd + shift - 1 : chunkc tiling::window --send-to-desktop 1
cmd + shift - 2 : chunkc tiling::window --send-to-desktop 2 
cmd + shift - 3 : chunkc tiling::window --send-to-desktop 3
cmd + shift - 4 : chunkc tiling::window --send-to-desktop 4
cmd + shift - 5 : chunkc tiling::window --send-to-desktop 5
cmd + shift - 6 : chunkc tiling::window --send-to-desktop 6
cmd + shift - 7 : chunkc tiling::window --send-to-desktop 7
cmd + shift - 8 : chunkc tiling::window --send-to-desktop 8
cmd + shift - 9 : chunkc tiling::window --send-to-desktop 9

cmd + alt - a : chunkc tiling::desktop --layout bsp
cmd + alt - s : chunkc tiling::desktop --layout monocle
cmd + alt - d : chunkc tiling::desktop --layout float

# Move around floats
cmd + alt - left   : chunkc tiling::window --grid-layout 1:2:0:0:1:1
cmd + alt - right  : chunkc tiling::window --grid-layout 1:2:1:0:1:1
cmd + alt - up     : chunkc tiling::window --grid-layout 2:1:0:0:1:1
cmd + alt - down   : chunkc tiling::window --grid-layout 2:1:0:1:1:1

cmd + ctrl - left   : chunkc tiling::window --grid-layout 2:2:0:0:1:1
cmd + ctrl - right  : chunkc tiling::window --grid-layout 2:2:1:0:1:1
cmd + ctrl + shift - left   : chunkc tiling::window --grid-layout 2:2:0:1:1:1
cmd + ctrl + shift - right  : chunkc tiling::window --grid-layout 2:2:1:1:1:1

cmd + alt - f       : chunkc tiling:window --grid-layout 1:1:0:0:1:1
